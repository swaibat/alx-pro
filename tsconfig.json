{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./src/*"],
      "@ui": ["./src/components"]
    }
  },
  "include": [
    "./src/**/*.js",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/app/(tabs)/_layout.js",
    "src/app/_layout.js", "src/components/@ui/AppImg.js" ]
}
